{
  "name": "judge-finder-platform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run validate:env && next dev",
    "dev:turbo": "next dev --turbo",
    "build": "npm run validate:env && next build",
    "build:production": "npm run validate:env && next build",
    "build:netlify": "next build",
    "start": "next start",
    "setup": "npm install && npm run validate:env",
    "clean": "rm -rf .next node_modules/.cache",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,yaml,yml}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,yaml,yml}\"",
    "validate:env": "node scripts/validate-env.js",
    "precommit": "lint-staged",
    "test": "vitest run",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:ci": "npm run lint && npm run type-check && npm run test:coverage",
    "lint": "eslint .",
    "lint:strict": "eslint . --max-warnings=0",
    "lint:models": "eslint lib/**/*.ts --max-warnings=0",
    "type-check": "tsc --noEmit",
    "integrity:full": "node scripts/database-integrity-check.js",
    "assignments:update": "node scripts/automated-assignment-updater.js run",
    "validate:relationships": "node scripts/run-validation.js",
    "sync:courts": "node scripts/sync-courts-manual.js",
    "sync:judges": "node scripts/sync-judges-manual.js",
    "sync:decisions": "node scripts/sync-decisions-manual.js",
    "import:wcab": "node scripts/import-wcab-judges.js",
    "analytics:generate": "node scripts/batch-generate-analytics.js",
    "bias:analyze": "node scripts/run-bias-analysis.js",
    "audit:court-slugs": "npx ts-node --skip-project --transpile-only --compiler-options '{\"module\":\"CommonJS\",\"moduleResolution\":\"node\"}' scripts/audit-court-slugs.ts",
    "test:normalization": "node scripts/validate-normalization.cjs",
    "cron:daily": "node scripts/run-daily-sync.js",
    "cron:weekly": "node scripts/run-weekly-sync.js",
    "data:status": "node scripts/check-data-status.js",
    "import:federal": "node scripts/import-us-federal-judges.js",
    "launch:analytics": "npm run analytics:generate",
    "launch:data": "npm run sync:courts && npm run sync:judges",
    "launch:validate": "npm run validate:relationships && npm run integrity:full",
    "stripe:sync-catalog": "ts-node --project tsconfig.json scripts/stripe/sync-catalog.ts",
    "template:api": "node scripts/template-copy.mjs api",
    "template:component": "node scripts/template-copy.mjs component",
    "template:sync": "node scripts/template-copy.mjs sync",
    "test:mcp": "node scripts/test-mcp.js",
    "test:analytics:modules": "ts-node --transpile-only scripts/test-analytics-modules.ts",
    "test:analytics:endpoints": "node scripts/test-analytics-endpoints.js",
    "diagnose:analytics": "node scripts/diagnose-analytics-issues.js",
    "validate-production-data": "ts-node --transpile-only scripts/validate-production-data.ts",
    "cleanup-production-data": "ts-node --transpile-only scripts/cleanup-production-data.ts",
    "backup-database": "bash scripts/backup-database.sh",
    "test-migration": "ts-node --transpile-only scripts/test-migration.ts",
    "benchmark:performance": "ts-node --transpile-only scripts/benchmark-performance.ts",
    "prepare": "node .husky/install.mjs"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.31.3",
    "@google/generative-ai": "^0.21.0",
    "@headlessui/react": "^1.7.18",
    "@heroicons/react": "^2.1.1",
    "@radix-ui/react-slot": "^1.2.3",
    "@sentry/nextjs": "^8.20.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.39.0",
    "@upstash/ratelimit": "^1.0.0",
    "@upstash/redis": "^1.32.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "d3": "^7.9.0",
    "dotenv": "^17.2.1",
    "framer-motion": "^12.23.12",
    "jose": "^6.1.0",
    "lru-cache": "^11.0.2",
    "lucide-react": "^0.539.0",
    "mobx": "^6.15.0",
    "mobx-react-lite": "^4.1.1",
    "next": "^15.5.3",
    "next-themes": "^0.4.6",
    "node-cron": "^3.0.3",
    "node-fetch": "^2.7.0",
    "openai": "^4.104.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-hook-form": "^7.49.3",
    "react-intersection-observer": "^9.16.0",
    "react-joyride": "^2.9.3",
    "react-virtualized-auto-sizer": "^1.0.11",
    "react-window": "^1.8.11",
    "recharts": "^3.2.1",
    "stripe": "^14.0.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.25.76",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@netlify/plugin-nextjs": "^5.13.3",
    "@playwright/test": "^1.49.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/d3": "^7.4.3",
    "@types/node": "^20.11.30",
    "@types/react": "^18.3.25",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.35.0",
    "eslint-config-next": "^15.5.3",
    "eslint-config-prettier": "^10.1.8",
    "happy-dom": "^19.0.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "module-alias": "^2.2.3",
    "p-limit": "^3.1.0",
    "pg": "^8.16.3",
    "prettier": "^3.6.2",
    "sharp": "^0.34.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": "^20",
    "npm": ">=10"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,mjs,cjs}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yaml,yml}": [
      "prettier --write"
    ]
  }
}
